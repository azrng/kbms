<script>
// check if assistant is ready:
$(function () {
  if (screen.width >= 1024 && location.pathname.startsWith('/wiki/') && window.g_user) {
    getJSON('/api/external/gpt/ratelimit', function (err, data) {
      if (data && !data.result) {
        window._CHAT_BOT_LOADED = function () {
          let $btn = $('div.x-assistant');
          $btn.click(function () {
            $btn.hide();
            window._CHAT_BOT.init();
          });
          $btn.show();
        };
        let s = document.createElement('script');
        s.type = 'text/javascript';
        s.async = true;
        s.src = 'https://liaoxuefeng.gitee.io/chatbot/chat-assistant.js';
        $('head').append(s);
      }
   });
  }
});
</script>
